<UserControl x:Class="NorthHorizon.Samples.InpcTemplate.Tests.Live.ViewElementTests.TestView2"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:inpc="http://schemas.northhorizon.net/samples/inpctemplate"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <UniformGrid Columns="1">
        <TextBlock Text="{Binding Type}" Background="LightBlue"/>
        <GroupBox Header="new DataContext" DataContext="{x:Null}">
            <TextBlock Text="{inpc:RootViewModelBinding Type}" Background="LightBlue"/>
        </GroupBox>
    </UniformGrid>
</UserControl>
